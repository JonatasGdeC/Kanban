@inherits LayoutComponentBase
<MudThemeProvider/>
<MudPopoverProvider/>
<MudDialogProvider/>
<MudSnackbarProvider/>

<MudLayout>
  <Header Board="@selectedBoard"/>
  <NavManu OnBoardSelected="HandleBoardSelected"/>
  <MudMainContent>
    @Body
  </MudMainContent>
</MudLayout>

@code {
  private BoardsResponse? selectedBoard;

  private void HandleBoardSelected(BoardsResponse? board)
  {
    selectedBoard = board;
  }
}
